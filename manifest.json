{
  "manifest_version": 2,
  "name": "Loafing Blocker",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "icons": { "48": "icon48.png", "128": "icon128.png" },
  "browser_action": {
    "default_popup": "newtab.html",
    "default_title": "Loafing Blocker"
  }

}
